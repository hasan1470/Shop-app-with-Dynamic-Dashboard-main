    <script>
      // data table
      $(".product-table").DataTable();

      // preload product images
      const product_image = document.querySelector("#product-photo");
      const product_image_preview = document.querySelector(".preload-product");

      if (product_image) {
        product_image.onchange = (e) => {
          const imageLink = URL.createObjectURL(e.target.files[0]);
          product_image_preview.setAttribute("src", imageLink);
        };
      }

      // preload slider images
      const slider_image = document.querySelector("#sliderphoto");
      const slider_photo_preview = document.querySelector(".preload-slider");

      if (slider_image) {
        slider_image.onclick = (e) => {
          const simagelink = URL.createObjectURL(e.target.files[0]);
          slider_photo_preview.setAttribute("src", simagelink);
        };
      }

      // delete button manage
      const delete_btn = document.querySelectorAll(".btn-detele");
      if (delete_btn) {
        delete_btn.forEach((item) => {
          item.onclick = () => {
            const conf = confirm("Are you sure?");

            if (conf) {
              return true;
            } else {
              return false;
            }
          };
        });
      }
    </script>
